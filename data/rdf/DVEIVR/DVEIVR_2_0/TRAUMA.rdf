    >radiation burn</skos:prefLabel>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Burn"/>
    </rdfs:subClassOf>
    <CORE:definitionSource rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
